/***************************************************************************
* style of QCheckBox
****************************************************************************/
QCheckBox
{
    color: #ECECEC;/*rgb(0,255,0);更改字体和选框*/
}
QCheckBox::indicator:checked {
	background-color:       blue;
	border:                 3px solid white;	
}

/***************************************************************************
* style of QProgressBar
****************************************************************************/
QProgressBar
{
    text-align: center;
}
QProgressBar::chunk {
    background-color: DodgerBlue;    
	border: 1px solid ;	
	border-radius: 3px;
}

/****************************************************************************
* style of textEdit
*****************************************************************************/
QTextEdit
{
    background-color: #111111;
    color: #FFFFFF;
    border-width: 1px;
    border-style: solid;
    border-color: #3E3E3E;
}

QTextEdit[description="true"]{
    background-color: #111111;
    border-width: 1px;
    border-style: solid;
    border-color: #3E3E3E
}

/*****************************************************************************
*  style of QMessageBox
*****************************************************************************/
QMessageBox
{
    background-color: #252525;
}
QMessageBox QPushButton
{
    width: 97;
    height: 25
}

/******************************************************************************
* style setting of QComboBox
****************************************************************************/
QComboBox
{
    border: 1px solid gray;
    border-radius: 3px;
    padding: 1px 5px 1px 3px;
}

QComboBox QAbstractItemView
{
    color: #EAEAEA;
    background: #111111;
}

QComboBox QAbstractItemView::item
{
    height: 22px;
}

QComboBox:editable
{
    color: #EAEAEA;
    background: #111111;
}

QComboBox:!editable, QComboBox::drop-down:editable
{
    color: #EAEAEA;
    background: #111111;
}

/* QComboBox gets the "on" state when the popup is open */
QComboBox:!editable:on, QComboBox::drop-down:editable:on
{
    color: #EAEAEA;
    background: #111111;
}

QComboBox:on
{ /* shift the text when the popup opens */
    padding-top: 3px;
    padding-left: 4px;
}

QComboBox::drop-down
{
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    border-left-width: 0px;
    border-image: url(:/image/button.bmp);
    border-left-style: solid; /* just a single line */
    border-top-right-radius: 3px; /* same radius as the QComboBox */
    border-bottom-right-radius: 3px;
}

QComboBox::drop-down:hover
{
    border-image: url(:/image/buttonHover.bmp);
}

QComboBox::drop-down:pressed
{
    border-image: url(:/image/buttonHover.bmp);
}

QComboBox::drop-down:disabled
{
    border-image: url(:/image/disable.bmp);
}

QComboBox::down-arrow
{
    width: 64px;
    image: url(:/image/down_arrow.png);
}

QComboBox::down-arrow:on
{ /* shift the arrow when popup is open */
    top: 1px;
    left: 1px;
}
QComboBox#coBox_cameraChooser,#comboBox_readmode:focus
{
    border-color:rgb(60,157,254);
}




