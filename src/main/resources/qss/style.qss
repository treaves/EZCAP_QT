/************************************************************************
* style of QMenuBar
************************************************************************/
QMenuBar
{
    background-color: rgb(48, 48, 48);
    color: rgb(255, 255, 255);
}
QMenuBar::item
{
    spacing: 3px; /* spacing between menu bar items */
    padding: 2px 4px;
    background: transparent;
    border-radius: 2px;
}
QMenuBar::item:selected
{
    background: #0588CA;
}
QMenuBar::item:pressed
{
    background: #0588CA;
}
QMenuBar::item:disabled
{
    color: rgb(134, 134, 134);
}

QMenu
{
    background-color: rgb(51, 51, 51);
    border: 1px solid rgb(90, 90, 90);
    border-radius: 4px;
    color: rgb(255, 255, 255);
    padding: 3px;
}
QMenu::item
{
    /* sets background of menu item. set this to something non-transparent
     if you want menu color and menu item color to be different */
    background-color: transparent;
    border: 1px solid transparent;
    padding: 2px 25px 2px 20px;
}
QMenu::item:selected
{
    /* when user selects item using mouse or keyboard */
    background-color: #0588CA;
    border-radius: 3px;
}
QMenu::item:disabled
{
    color: rgb(134, 134, 134);
}

QMenu::separator
{
    height: 2px;
    margin-left: 10px;
    margin-right: 5px;
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(0, 0, 0, 255), stop:0.6 rgba(0, 0, 0, 255), stop:0.7 rgba(83, 83, 83, 255), stop:1 rgba(83, 83, 83, 255));
}

